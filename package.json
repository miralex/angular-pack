{
  "name": "angular-pack",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:prod": "npm run build:prod && npm run server",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "ng lint && ng test --configuration=test",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch",
    "ci": "npm run test && npm run e2e && npm run build:prod -- --deploy-url /angular-pack/ --base-href /angular-pack",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/angular-pack/stats-es2015.json",
    "server": "node ./projects/server/server.js"
  },
